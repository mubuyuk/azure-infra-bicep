{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "swedencentral"
    },
    "environment": {
      "value": "prod"
    },
    "tags": {
      "value": {
        "owner": "murat",
        "environment": "prod",
        "costCenter": "bicep-labb"
      }
    },


    "storageSku": {
      "value": "Standard_LRS"
    },
    "storageKind": {
      "value": "StorageV2"
    },
    "deployStorage": {
      "value": true
    },


    "appServicePlanPrefix": {
      "value": "myAppPlan"
    },
    "webAppPrefix": {
      "value": "mywebapp"
    },
    "appServiceSku": {
      "value": "S1"
    },
    "httpsOnly": {
      "value": true
    },
    "isLinux": {
      "value": true
    },

    "enableAutoscale": { "value": true },
    "autoscaleMin": { "value": "1" },
    "autoscaleDefault": { "value": "1" },
    "autoscaleMax": { "value": "5" },
    "autoscaleCpuOut": { "value": 70 },
    "autoscaleCpuIn": { "value": 30 },
    "autoscaleTimeWindow": { "value": "PT5M" },
    "autoscaleTimeGrain": { "value": "PT1M" },
    "autoscaleCooldown": { "value": "PT5M" }
  }
}